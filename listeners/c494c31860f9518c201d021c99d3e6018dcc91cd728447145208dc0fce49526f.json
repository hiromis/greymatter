                  "principal_name": {
                    "safe_regex": {
                      "google_re2": {},
                      "regex": "spiffe://quickstart.greymatter.io/(.+)"
                    }
                  }
                }
              }
            ]
          },
          "product-viewer": {
            "permissions": [
              {
                "header": {
                  "name": ":method",
                  "exact_match": "GET"
