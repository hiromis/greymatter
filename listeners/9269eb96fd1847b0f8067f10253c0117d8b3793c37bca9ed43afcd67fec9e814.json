{
  "access_loggers": {
    "http_connection_loggers": {
      "disabled": false,
      "file_loggers": null,
      "http_grpc_access_loggers": null
    },
    "http_upstream_loggers": {
      "disabled": false,
      "file_loggers": null,
      "http_grpc_access_loggers": null
    }
  },
  "active_http_filters": [
    "gm.metrics",
    "envoy.rbac"
  ],
  "active_network_filters": null,
  "checksum": "",
  "delayed_close_timeout": "",
  "domain_keys": [
    "domain-slo"
  ],
  "drain_timeout": "",
  "http2_protocol_options": null,
  "http_filters": {
    "envoy_rbac": {
      "rules": {
        "action": 0,
        "policies": {
          "service-admin": {
            "permissions": [
              {
                "any": true
              }
            ],
            "principals": [
              {
                "header": {
                  "name": "user_dn",
                  "exact_match": "CN=quickstart,OU=Engineering,O=Decipher Technology Studios,L=Alexandria,ST=Virginia,C=US"
                }
              }
            ]
          },
          "services": {
            "permissions": [
              {
                "any": true
              }
            ],
            "principals": [
              {
                "authenticated": {
                  "principal_name": {
                    "safe_regex": {
                      "google_re2": {},
                      "regex": "spiffe://quickstart.greymatter.io/(.+)"
                    }
                  }
                }
              }
            ]
          },
          "product-viewer": {
            "permissions": [
              {
                "header": {
                  "name": ":method",
                  "exact_match": "GET"
                }
              }
            ],
            "principals": [
              {
                "any": true
              }
            ]
          }
        }
      }
    },
    "gm_metrics": {
      "metrics_port": 8081,
      "metrics_host": "0.0.0.0",
      "metrics_dashboard_uri_path": "/metrics",
      "metrics_prometheus_uri_path": "/prometheus",
      "metrics_ring_buffer_size": 4096,
      "prometheus_system_metrics_interval_seconds": 15,
      "metrics_key_function": "depth",
      "metrics_key_depth": "3"
    }
  },
  "http_protocol_options": null,
  "ip": "0.0.0.0",
  "listener_key": "listener-slo",
  "name": "slo",
  "network_filters": null,
  "port": 10808,
  "protocol": "http_auto",
  "request_timeout": "",
  "secret": {
    "secret_key": "secret-slo-secret",
    "secret_name": "spiffe://quickstart.greymatter.io/slo",
    "secret_validation_name": "spiffe://quickstart.greymatter.io",
    "subject_names": [
      "spiffe://quickstart.greymatter.io/edge"
    ],
    "ecdh_curves": [
      "X25519:P-256:P-521:P-384"
    ],
    "forward_client_cert_details": "APPEND_FORWARD",
    "set_current_client_cert_details": {
      "uri": true
    },
    "checksum": "cb981f53e1d87de576c67dc76c2719c525ec90b770e7992f826d93423859c206"
  },
  "stream_idle_timeout": "",
  "tracing_config": null,
  "use_remote_address": false,
  "zone_key": "zone-default-zone"
}